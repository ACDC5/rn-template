{
  "name": "rnTemplate-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "codePush": "node scripts/codePush/index.js",
    "clean": "cd ./android && ./gradlew clean",
    "android:release": "npm run clean && npm run jetifier && cd ./android && ./gradlew app:assembleRelease",
    "android:staging": "npm run clean && npm run jetifier && cd ./android && ./gradlew app:assembleReleaseStaging",
    "tsc": "tsc -p ./tsconfig.json",
    "eslint": "eslint --ext .ts,.tsx src/",
    "eslint:fix": "eslint --fix --ext .ts,.tsx src/",
    "lint": "prettier --check src/**/*.ts src/**/*.tsx",
    "lint:fix": "prettier --check src/**/*.tsx --write",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "release": "git add . && node release.js",
    "postrelease": "git push --follow-tags",
    "jetifier": "npx jetifier",
    "pod": "cd ios && pod install && cd .."
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "changelog": {
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run eslint:fix && npm run lint:fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^4.0.4",
    "@ant-design/icons-react-native": "^2.2.1",
    "@ant-design/react-native": "^4.0.2",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/cameraroll": "^4.0.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.5",
    "@react-native-community/picker": "^1.6.1",
    "@react-native-community/segmented-control": "^2.0.0",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@react-native-community/viewpager": "^4.1.3",
    "@react-navigation/bottom-tabs": "^5.7.3",
    "@react-navigation/native": "^5.7.2",
    "@react-navigation/stack": "^5.8.0",
    "@sentry/react-native": "^1.7.0",
    "ahooks": "^2.1.0",
    "dayjs": "^1.8.31",
    "immer": "^7.0.7",
    "jcore-react-native": "^1.8.0",
    "jpush-react-native": "^2.8.1",
    "lodash-es": "^4.17.15",
    "rc-field-form": "^1.8.0",
    "react": "^16.13.1",
    "react-native": "0.62.2",
    "react-native-background-timer": "^2.3.1",
    "react-native-canvas": "^0.1.37",
    "react-native-code-push": "^6.2.1",
    "react-native-config-reader": "^4.1.0",
    "react-native-device-info": "^5.6.3",
    "react-native-exit-app": "^1.1.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-picker": "^2.3.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-pay-password": "^1.0.1",
    "react-native-permissions": "^2.1.5",
    "react-native-reanimated": "^1.10.1",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^2.8.0",
    "react-native-settings": "^0.2.3",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-swipe-list-view": "^3.2.3",
    "react-native-switch": "^2.0.0",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "8.1.2",
    "react-native-wechat-lib": "^1.1.18",
    "rn-fetch-blob": "^0.12.0",
