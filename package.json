    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "inquirer": "^7.3.3",
    "jest": "^26.1.0",
    "jetifier": "^1.6.5",
    "lint-staged": "^10.2.9",
    "metro-react-native-babel-preset": "^0.60.0",
    "pont-engine": "^1.0.3",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "shelljs": "^0.8.4",
    "standard-version": "^8.0.2",
    "typescript": "^3.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
